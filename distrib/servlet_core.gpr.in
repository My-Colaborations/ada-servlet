with "util";
with "util_config";
with "el";
with "security";

project Servlet_Core is
   Version := "@SERVLET_VERSION@";

   for Library_Name use "ada_servlet";
   for Library_Dir use Util_Config.Libdir;
   for Library_Kind use "LIBRARY_TYPE";

   for Source_Dirs use (Util_Config.Includedir & "/servlet");
   for Library_ALI_Dir use Util_Config.Alidir & "/servlet/";
   for Externally_Built use "true";
end Servlet_Core;
